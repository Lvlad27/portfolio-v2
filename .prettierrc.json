{
  "trailingComma": "es5",
  "singleQuote": true,
  "semi": true,
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "bracketSpacing": true,
  "arrowParens": "always",
  "endOfLine": "auto",
  "importOrder": [
    "^react$",
    "<THIRD_PARTY_MODULES",
    "^components/(.*)$",
    "^@public/",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSpecifiers": true
}
